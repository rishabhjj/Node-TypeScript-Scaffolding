{
	"extends": [
		"tslint:recommended",
		"tslint-config-airbnb"
	],
	"jsRules": {},
	"rulesDirectory": [],
	"linterOptions": {
		"exclude": [
			"build/**/*.js",
			"node_modules/**/*.*"
		]
	},
	"rules": {
		"no-var-requires": false,
		"no-duplicate-imports": false,
		"object-literal-sort-keys": false,
		"no-console": false,
		"variable-name": false,
		"max-line-length": false,
		"ordered-imports" : false,
		"function-name": false,
		"import-name": false,
		"no-parameter-reassignment": false,
		"align": false,
		"label-position": false
	}
}